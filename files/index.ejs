<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Grand Invest</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="css/styles.css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://kit.fontawesome.com/f5c7d6a886.js" crossorigin="anonymous"></script>
    <link rel="icon" type="image/x-icon" href="img/city-solid.svg">
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<body style="background-color: rgb(200,200,200);">
<!-- NAVBAR -->
<nav class="navbar navbar-expand-sm bg-dark navbar-dark">
    <div class="container-fluid">
        <a class="navbar-brand" href="index">
            <i class="fa-solid fa-house"></i>
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#collapsibleNavbar">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="collapsibleNavbar">
            <ul class="navbar-nav">
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown">Обзор</a>
                    <ul class="dropdown-menu">
                        <li><a class="dropdown-item" href="#">Продано</a></li>
                        <li><a class="dropdown-item" href="#">Забронировано</a></li>
                        <li><a class="dropdown-item" href="#">Активно</a></li>
                    </ul>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="adder">Adding tool</a>
                </li>
            </ul>
        </div>
    </div>
</nav>
<!-- END OF NAVBAR -->

<!-- FILTER -->
<div class="container-sm pt-2 pb-2 px-4 search">
    <!-- FIRST ROW OF FILTER -->
    <div class="row">
        <!-- HOUSE CHOICE -->
        <div class="col-lg-3 border-end border-secondary d-flex align-items-center">
            <div class="row">
                <div class="col-lg-6" >
                    <label>Выбрать ЖК</label>
                </div>
                <div class="col-lg-6">
                    <label>
                        <select>
                            <option value="">Выбрать</option>
                            <option value="Nova">Нова</option>
                            <option value="Esil">Есиль</option>
                            <option value="Vremena goda">Времена года</option>
                        </select>
                    </label>
                </div>
            </div>
        </div>
        <!-- END OF HOUSE CHOICE -->

        <!-- ROOM CHOICE -->
        <div class="col-lg-3 border-end border-secondary d-flex align-items-center">
            <div class="row">
                <div class="col-lg-3 ps-3 pt-2">
                    <label>Комнат</label>
                </div>
                <div class="col-lg-4">
                    <label class="pt-1">
                        <input class="form-control px-2" max="500" min="0" placeholder="От">
                    </label>
                </div>
                <div class="col-lg-1 pe-1 ps-1 pt-2">
                    <label>—</label>
                </div>
                <div class="col-lg-4">
                    <label class="pt-1">
                        <input class="form-control px-2" max="500" min="0" placeholder="До">
                    </label>
                </div>
            </div>
        </div>
        <!-- END OF ROOM CHOICE -->

        <!-- FLOOR CHOICE -->
        <div class="col-lg-3 border-end border-secondary d-flex align-items-center">
            <div class="row">
                <div class="col-lg-3 ps-4  pt-2" >
                    <label>Этаж</label>
                </div>
                <div class="col-lg-4">
                    <label class="pt-1">
                        <input class="form-control px-2" max="500" min="0" placeholder="От">
                    </label>
                </div>
                <div class="col-lg-1 pt-2 ps-1 pe-1">
                    <label>—</label>
                </div>
                <div class="col-lg-4">
                    <label class="pt-1">
                        <input class="form-control px-2" max="500" min="0" placeholder="До">
                    </label>
                </div>
            </div>
        </div>
        <!-- END OF FLOOR CHOICE -->

        <!-- AREA CHOICE -->
        <div class="col-lg-3 d-flex align-items-center">
            <div class="row">
                <div class="col-lg-4 pe-0 pt-2" >
                    <label>Площадь, м²</label>
                </div>
                <div class="col-lg-3 pe-1">
                    <label class="pt-1">
                        <input class="form-control px-2" placeholder="От">
                    </label>
                </div>
                <div class="col-lg-1 pt-2 ps-2 pe-1">
                    <label>—</label>
                </div>
                <div class="col-lg-4">
                    <label class="pt-1">
                        <input class="form-control px-2" placeholder="До">
                    </label>
                </div>
            </div>
        </div>
        <!-- END OF AREA CHOICE -->
    </div>
    <!-- END OF FIRST ROW OF FILTER -->

    <!-- SECOND ROW OF FILTER -->
    <div class="row mt-3">
        <!-- PRICE CHOICE -->
        <div class="col-lg-6 border-end border-secondary">
            <div class="row">
                <div class="col-lg-3 pt-2 ps-5" >
                    <label>Цена, тг</label>
                </div>
                <div class="col-lg-3 pe-0 ps-0">
                    <label class="pt-1">
                        <input class="form-control px-2" value="" id="costMoreID" placeholder="От" style="font-size: 14px">
                    </label>
                </div>
                <div class="col-lg-auto pt-2">
                    <label>—</label>
                </div>
                <div class="col-lg-4 ps-0 pe-0">
                    <label class="pt-1">
                        <input class="form-control px-2" placeholder="До" style="font-size: 14px">
                    </label>
                </div>
            </div>
        </div>
        <!-- END OF PRICE CHOICE -->

        <!-- FLAT NUMBER CHOICE -->
        <div class="col-lg-4 border-end border-secondary d-flex align-items-center">
            <div class="row">
                <div class="col-lg-6 ps-5 pt-2">
                    <label>Квартира №</label>
                </div>
                <div class="col-lg-6">
                    <label class="pt-1">
                        <input class="form-control px-2" placeholder="">
                    </label>
                </div>
            </div>
        </div>
        <!-- END OF FLAT NUMBER CHOICE -->

        <!-- SEARCH BUTTON -->
        <div class="col-lg-2 d-flex justify-content-center">
            <button class="btn btn-primary">ПОИСК</button>
        </div>
        <!-- END OF SEARCH BUTTON -->
    </div>
    <!-- END OF SECOND ROW OF FILTER -->
</div>
<!-- END OF FILTER -->

<!-- START OF PRODUCTION -->
